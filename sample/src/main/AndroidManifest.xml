<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sxdsf.mvpit.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".onepresenter.activity.OnePresenterActivity" />
        <activity android:name=".multipresenter.activity.MultiPresenterActivity" />
        <activity android:name=".onepresenter.fragment.OneFragmentActivity" />
        <activity android:name=".multipresenter.fragment.MultiFragmentActivity"></activity>
    </application>

</manifest>
